        "Criei outro snapshot ## sp_shipment.sql ##\n",
        "\n",
        "\n",
        "{% snapshot sp_shipment %}\n",
        "\n",
        "{{\n",
        "    config(\n",
        "        target_schema='snapshots',\n",
        "        unique_key='ship_method_id',\n",
        "        strategy='timestamp',\n",
        "        updated_at='modified_date'\n",
        "    )\n",
        "}}\n",
        "\n",
        "select\n",
        "    *\n",
        "from {{ ref('stg_shipment') }}\n",
        "\n",
        "{% endsnapshot %}"
